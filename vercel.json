{
  "redirects": [
    {
      "source": "https://www.figurefinance.co/:path*",
      "destination": "https://figurefinance.co/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}
